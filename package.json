{
  "name": "romodel",
  "version": "0.0.9",
  "description": "RoModel is a simple, lightweight, unopinionated, read-only, immutable, model based data access library",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "build-watch": "babel src --watch --out-dir lib",
    "start": "npm run build && npm run test-build && npm run test",
    "test-build": "babel test --out-dir testout",
    "test-build-watch": "babel test --watch --out-dir testout",
    "test": "mocha testout/**/*.js",
    "test-watch": "mocha --watch testout/**/*.js",
    "watch": "npm run build-watch & npm run test-build-watch & npm run test-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/romodel.git"
  },
  "keywords": [
    "romodel",
    "model",
    "lightweight",
    "readonly",
    "immutable",
    "data",
    "access",
    "library"
  ],
  "author": "Joon Ho Cho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joonhocho/romodel/issues"
  },
  "homepage": "https://github.com/joonhocho/romodel#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^2.4.5"
  }
}
